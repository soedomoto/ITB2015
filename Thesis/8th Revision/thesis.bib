	author = {Zheng, Baihua and Xu, Jianliang and Lee, Wang-Chien and Lee, Lun},
	month = jan,
	year = {2006},
	keywords = {Continuous-nearest-neighbor search, Index structure, Location-dependent data, Nearest-neighbor search, Wireless broadcast},
	pages = {21--39},
