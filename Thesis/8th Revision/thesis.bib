	pages = {1310--1317},
	file = {IEEE Xplore Abstract Record:/home/soedomoto/.zotero/zotero/gu987of5.default/zotero/storage/CME6FJK4/abs_all.html:text/html;IEEE Xplore Full Text PDF:/home/soedomoto/.zotero/zotero/gu987of5.default/zotero/storage/JFXZPJSF/Wang et al. - 2013 - Locality Based Data Partitioning in MapReduce.pdf:application/pdf}
