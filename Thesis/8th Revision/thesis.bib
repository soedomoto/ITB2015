	keywords = {peer-to-peer, replication, search, unstructured},
	pages = {84--95},
	file = {ACM Full Text PDF:/home/soedomoto/.zotero/zotero/gu987of5.default/zotero/storage/6WPXVVG6/Lv et al. - 2002 - Search and Replication in Unstructured Peer-to-pee.pdf:application/pdf}
