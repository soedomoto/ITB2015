	author = {DeRenzi, Brian and Anokwa, Yaw and Parikh, Tapan and Borriello, Gaetano},
	year = {2007},
	pages = {4:1--4:5},
	file = {ACM Full Text PDF:/media/soedomoto/DATA/BIBLIOGRAPHY/storage/A5NQ74EC/DeRenzi et al. - 2007 - Reliable Data Collection in Highly Disconnected En.pdf:application/pdf}
}

@inproceedings{zottl_three-tier_2008,
