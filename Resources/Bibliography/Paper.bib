@inproceedings{yao_proxy-based_2000,
	title = {Proxy-based recovery for applications on wireless hand-held devices},
	doi = {10.1109/RELDI.2000.885387},
	abstract = {The low communication bandwidth, slow processor and limited memory of hand-held devices make it undesirable for them to store their own checkpoints or send process state information over a wireless network. The paper describes an approach to failure recovery for three-tier client and server application environments where the client applications execute on wireless handheld devices. The key idea is to have the middle-tier proxy transparently monitor the client's interaction with the back-end server and continuously maintain a copy of the client's state based on messages exchanged between the client and the server. The proxy also sustains the client's connection to the back-end server when a client unexpectedly disconnects. The client does not participate in checkpointing nor message logging, thereby saving power, processor cycles and bandwidth. The proxy is scalable and enhances backend server performance. Experimental results are provided for recovery time and runtime overhead},
	booktitle = {Proceedings {The} 19th {IEEE} {Symposium} on {Reliable} {Distributed} {Systems}, 2000. {SRDS}-2000},
	author = {Yao, Bin and Fuchs, W. K.},
	year = {2000},
	keywords = {Application software, back-end server, backend server performance, bandwidth, checkpoints, client interaction, client-server systems, client state, communication bandwidth, Computer crashes, Contracts, failure recovery, fault tolerant computing, Handheld computers, message exchange, middle-tier proxy, Mobile computing, Network servers, notebook computers, processor cycles, process state information, proxy based recovery, radiocommunication, Random access memory, recovery time, Runtime, runtime overhead, three-tier client/server application environments, Wireless communication, wireless hand-held device applications, wireless network, Wireless networks},
	pages = {2--10},
	file = {IEEE Xplore Abstract Record:/media/soedomoto/DATA/BIBLIOGRAPHY/storage/CBWZ6HZ9/885387.html:text/html;IEEE Xplore Full Text PDF:/media/soedomoto/DATA/BIBLIOGRAPHY/storage/NXRXJ4TU/Yao and Fuchs - 2000 - Proxy-based recovery for applications on wireless .pdf:application/pdf}
