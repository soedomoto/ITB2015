@inproceedings{datta_proxy-based_2002,
	address = {New York, NY, USA},
	series = {{SIGMOD} '02},
	title = {Proxy-based {Acceleration} of {Dynamically} {Generated} {Content} on the {World} {Wide} {Web}: {An} {Approach} and {Implementation}},
