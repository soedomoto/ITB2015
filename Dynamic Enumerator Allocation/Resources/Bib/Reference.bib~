@article{acosta_two-level_2015,
	title = {Two-{Level} {Software} {Architecture} for {Context}-{Aware} {Mobile} {Distributed} {Systems}},
	volume = {13},
	issn = {1548-0992},
	doi = {10.1109/TLA.2015.7106376},
	abstract = {Currently, there is a trend to develop context-aware mobile distributed systems (MDS), such as systems that recommend places according to the location and the path of users. Some of the key challenges in the development of this type of systems are the following: acquisition, management and use of data context. In this paper, we propose two-level software architecture to obtain, use and provide context information in a MDS. From our point of view, the proposed architecture takes into account the requirements that emerge from data collection, use and management of context, as well as the own features of MDS. Based on our software architecture, a context-aware group communication system is implemented, which allows members to have four types of context: physical activity, logical activity, location and level of sound.},
	number = {4},
	journal = {IEEE Latin America Transactions},
	author = {Acosta, M. A. Medina and Dominguez, E. Lopez and Castro, G. Gomez and Hernandez, S. E. Pomares and Nieto, M. A. Medina},
	month = apr,
	year = {2015},
	keywords = {Androids, Context, Context-aware Applications, context-aware group communication system, context aware mobile distributed systems, Context-aware services, context information, data collection, data context, data handling, distributed processing, Humanoid robots, logical activity, MDS, Mobile communication, Mobile computing, Mobile Distributed Systems, physical activity, Software, software architecture, two-level software architecture},
	pages = {1205--1209},
	file = {IEEE Xplore Abstract Record:/media/soedomoto/DATA/BIBLIOGRAPHY/storage/MI7ZZNKA/7106376.html:text/html;IEEE Xplore Full Text PDF:/media/soedomoto/DATA/BIBLIOGRAPHY/storage/GWUWQAJE/Acosta et al. - 2015 - Two-Level Software Architecture for Context-Aware .pdf:application/pdf}
