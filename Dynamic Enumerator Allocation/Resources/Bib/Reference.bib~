@inproceedings{yuan_location_2016,
	title = {Location recommendation algorithm based on temporal and geographical similarity in location-based social networks},
	doi = {10.1109/WCICA.2016.7578804},
	abstract = {Because the existing location recommendation algorithms in Location-based Social Networks have the characteristic of high time complexity and low recommendation accuracy, a new location recommendation algorithm based on temporal and geographical similarity is proposed by improving the traditional location recommendation algorithm in this paper. New recommendation algorithm has innovation mainly in the following three aspects: At first, new algorithm changes the traditional processing method of time dimension, it divides 24 hours into some periods of time in accordance with the time law of people's work and life, so the user similarity calculated by such periods of time will be more accurate; Secondly, the DBSCAN algorithm is improved by introducing grid thought, which makes the clustering object is no longer a single check-in point, but a grid contained a lot of check-in points, this improves the speed of recommendation algorithm greatly; Finally, a new rating function of the potential points of interest which are never visited by the user is proposed. The experimental results show that the proposed approach can improve the speed and precision of recommendation system obviously.},
	booktitle = {2016 12th {World} {Congress} on {Intelligent} {Control} and {Automation} ({WCICA})},
	author = {Yuan, Z. and Li, H.},
	month = jun,
	year = {2016},
	keywords = {Business, Clustering algorithms, Clustering methods, DBSCAN algorithm, geographical similarity, Lenses, location-based social networks, location recommendation algorithm, Mobile communication, pattern clustering, rating function, Recommender systems, social networking (online), Social network services, temporal similarity, Time complexity},
	pages = {1697--1702},
	file = {IEEE Xplore Abstract Record:/media/soedomoto/DATA/BIBLIOGRAPHY/storage/2WXR5J23/7578804.html:text/html;IEEE Xplore Full Text PDF:/media/soedomoto/DATA/BIBLIOGRAPHY/storage/JF7MR8VA/Yuan and Li - 2016 - Location recommendation algorithm based on tempora.pdf:application/pdf}
