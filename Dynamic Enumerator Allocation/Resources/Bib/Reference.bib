@inproceedings{yuan_location_2016,
	title = {Location recommendation algorithm based on temporal and geographical similarity in location-based social networks},
	doi = {10.1109/WCICA.2016.7578804},
	abstract = {Because the existing location recommendation algorithms in Location-based Social Networks have the characteristic of high time complexity and low recommendation accuracy, a new location recommendation algorithm based on temporal and geographical similarity is proposed by improving the traditional location recommendation algorithm in this paper. New recommendation algorithm has innovation mainly in the following three aspects: At first, new algorithm changes the traditional processing method of time dimension, it divides 24 hours into some periods of time in accordance with the time law of people's work and life, so the user similarity calculated by such periods of time will be more accurate; Secondly, the DBSCAN algorithm is improved by introducing grid thought, which makes the clustering object is no longer a single check-in point, but a grid contained a lot of check-in points, this improves the speed of recommendation algorithm greatly; Finally, a new rating function of the potential points of interest which are never visited by the user is proposed. The experimental results show that the proposed approach can improve the speed and precision of recommendation system obviously.},
	booktitle = {2016 12th {World} {Congress} on {Intelligent} {Control} and {Automation} ({WCICA})},
	author = {Yuan, Z. and Li, H.},
	month = jun,
	year = {2016},
	keywords = {Business, Clustering algorithms, Clustering methods, DBSCAN algorithm, geographical similarity, Lenses, location-based social networks, location recommendation algorithm, Mobile communication, pattern clustering, rating function, Recommender systems, social networking (online), Social network services, temporal similarity, Time complexity},
	pages = {1697--1702},
	file = {IEEE Xplore Abstract Record:/media/soedomoto/DATA/BIBLIOGRAPHY/storage/2WXR5J23/7578804.html:text/html;IEEE Xplore Full Text PDF:/media/soedomoto/DATA/BIBLIOGRAPHY/storage/JF7MR8VA/Yuan and Li - 2016 - Location recommendation algorithm based on tempora.pdf:application/pdf}
}

@inproceedings{zhu_context-aware_2016,
	title = {Context-aware location recommendations with tensor factorization},
	doi = {10.1109/ICCChina.2016.7636832},
	abstract = {Location-based social networks (LBSNs) enable users not only to record where and when they go using check-ins but also to publish their ratings and comments of these locations. All information can be utilized to infer users' interests and to generate personalized location recommendations for different individuals. In this process, temporal context plays a significant role, because life patterns of users vary greatly and different locations have different proper visiting time. Therefore, in this paper, we propose a top-k location recommendation scheme, considering the influence of temporal context. To be specific, we first model user-location-time relations with a three dimensional tensor. Then, we extract a user-user similarity matrix and a location feature matrix to relieve data sparsity problem and to increase the prediction accuracy of estimated ratings during the tensor decomposition process. Through a context-aware tensor decomposition approach, we can obtain any user's rating for any location in any time slot. Moreover, we partition a large area into several small ones and adopt the Threshold Algorithm (TA) to improve efficiency of online recommendations. We evaluate our method in a real-world dataset and the results demonstrate the effectiveness of our method.},
	booktitle = {2016 {IEEE}/{CIC} {International} {Conference} on {Communications} in {China} ({ICCC})},
	author = {Zhu, X. and Hao, R.},
	month = jul,
	year = {2016},
	keywords = {Bars, check-ins, collaboration, Context, context-aware location recommendation, context-aware tensor decomposition approach, data sparsity problem, Feature extraction, History, LBSN, location-based social network, location-based social networks, location feature matrix, Location recommendations, matrix decomposition, social networking (online), Temporal context, Tensile stress, Tensor decomposition, tensor factorization, tensors, threshold algorithm, top-k location recommendation scheme, ubiquitous computing, user-user similarity matrix},
	pages = {1--6},
	file = {IEEE Xplore Abstract Record:/media/soedomoto/DATA/BIBLIOGRAPHY/storage/PZTEFZ2R/7636832.html:text/html;IEEE Xplore Full Text PDF:/media/soedomoto/DATA/BIBLIOGRAPHY/storage/63V6Z4MK/Zhu and Hao - 2016 - Context-aware location recommendations with tensor.pdf:application/pdf}
}

