	pages = {993--998},
	file = {IEEE Xplore Abstract Record:/media/soedomoto/DATA/BIBLIOGRAPHY/storage/GWH4RESK/6644136.html:text/html;IEEE Xplore Full Text PDF:/media/soedomoto/DATA/BIBLIOGRAPHY/storage/DAPVGGUT/Bobek et al. - 2013 - Learning sensors usage patterns in mobile context-.pdf:application/pdf}
}

