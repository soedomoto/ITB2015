@article{wang_minmax_2016,
	title = {The min–max split delivery multi-depot vehicle routing problem with minimum service time requirement},
	volume = {71},
	issn = {0305-0548},
	url = {http://www.sciencedirect.com/science/article/pii/S0305054816000101},
	doi = {10.1016/j.cor.2016.01.008},
	abstract = {The min–max Split Delivery Multi-Depot Vehicle Routing Problem with Minimum Service Time Requirement (min–max SDMDVRP-MSTR) is a variant of the Multi-Depot Vehicle Routing Problem. Each customer requires a specified amount of service time. The service time can be split among vehicles as long as each vehicle spends a minimum amount of service time at a customer. The objective is to minimize the duration of the longest route (where duration is the sum of travel and service times).

We develop a heuristic (denoted by MDS) that solves the min–max SDMDVRP-MSTR in three stages: (1) initialize a feasible solution without splits; (2) improve the longest routes by splitting service times; (3) ensure all minimum service time requirements are satisfied. The first stage of MDS is compared to an existing heuristic to solve the min–max Multi-Depot Vehicle Routing Problem on 43 benchmark instances. MDS produces 37 best-known solutions. We also demonstrate the effectiveness of MDS on 21 new instances whose (near) optimal solutions can be estimated based on geometry. Finally, we investigate the savings from split service and the split patterns as we vary the required service times, the average number of customers per route, and the minimum service time requirement.},
	urldate = {2017-01-02},
	journal = {Computers \& Operations Research},
	author = {Wang, Xingyin and Golden, Bruce and Wasil, Edward and Zhang, Rui},
	month = jul,
	year = {2016},
	keywords = {Minimum service time requirement, Min–max, Multi-depot, Service times, Split delivery, Vehicle routing},
	pages = {110--126},
	file = {ScienceDirect Full Text PDF:/media/soedomoto/DATA/BIBLIOGRAPHY/storage/U5D4GPXK/Wang et al. - 2016 - The min–max split delivery multi-depot vehicle rou.pdf:application/pdf;ScienceDirect Snapshot:/media/soedomoto/DATA/BIBLIOGRAPHY/storage/T259M3Q6/S0305054816000101.html:text/html}
}

@article{dondo_cluster-based_2007,
