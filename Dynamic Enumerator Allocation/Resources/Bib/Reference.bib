
@article{acosta_two-level_2015,
	title = {Two-{Level} {Software} {Architecture} for {Context}-{Aware} {Mobile} {Distributed} {Systems}},
	volume = {13},
	issn = {1548-0992},
	doi = {10.1109/TLA.2015.7106376},
	abstract = {Currently, there is a trend to develop context-aware mobile distributed systems (MDS), such as systems that recommend places according to the location and the path of users. Some of the key challenges in the development of this type of systems are the following: acquisition, management and use of data context. In this paper, we propose two-level software architecture to obtain, use and provide context information in a MDS. From our point of view, the proposed architecture takes into account the requirements that emerge from data collection, use and management of context, as well as the own features of MDS. Based on our software architecture, a context-aware group communication system is implemented, which allows members to have four types of context: physical activity, logical activity, location and level of sound.},
	number = {4},
	journal = {IEEE Latin America Transactions},
	author = {Acosta, M. A. Medina and Dominguez, E. Lopez and Castro, G. Gomez and Hernandez, S. E. Pomares and Nieto, M. A. Medina},
	month = apr,
	year = {2015},
	keywords = {Androids, Context, Context-aware Applications, context-aware group communication system, context aware mobile distributed systems, Context-aware services, context information, data collection, data context, data handling, distributed processing, Humanoid robots, logical activity, MDS, Mobile communication, Mobile computing, Mobile Distributed Systems, physical activity, Software, software architecture, two-level software architecture},
	pages = {1205--1209},
	file = {IEEE Xplore Abstract Record:/media/soedomoto/DATA/BIBLIOGRAPHY/storage/MI7ZZNKA/7106376.html:text/html;IEEE Xplore Full Text PDF:/media/soedomoto/DATA/BIBLIOGRAPHY/storage/GWUWQAJE/Acosta et al. - 2015 - Two-Level Software Architecture for Context-Aware .pdf:application/pdf}
}

@article{ye_development_2014,
	title = {Development of a {Highly} {Flexible} {Mobile} {GIS}-{Based} {System} for {Collecting} {Arable} {Land} {Quality} {Data}},
	volume = {7},
	issn = {1939-1404},
	doi = {10.1109/JSTARS.2014.2320635},
	abstract = {In recent years, well-designed terminal-based methods for collecting index data have gradually replaced traditional pen-and-paper methods and have been extensively used in numerous studies. These new approaches offer users increased accuracy, efficiency, consumption, and data compatibility compared to traditional methods. In general, we find that spatial data content and quality index systems vary widely across different arable land regions. Thus, a system for the investigation of arable land quality indices that has the flexibility to utilize various types of spatial data and quality indices without requiring program modification is needed. This paper presents the framework, the module partition, and the structure of the data exchange interface for a highly flexible mobile GIS-based system, which we call the “arable land quality index data collection system” (ALQIDCS). This system incorporates a series of self-adaptive methods, a data table-driven model and two types of formulas for flexible data collection and processing. We tested our prototype system by investigating arable land quality in the Da Xing District, Beijing and in the Te Da La Qi District, Inner Mongolia, China. The results indicate that the ALQIDCS can effectively adapt to variations in spatial data and quality index systems and meet different objectives. The limitations of the ALQIDCS and suggestions for future work are also presented.},
	number = {11},
	journal = {IEEE Journal of Selected Topics in Applied Earth Observations and Remote Sensing},
	author = {Ye, S. and Zhu, D. and Yao, X. and Zhang, N. and Fang, S. and Li, L.},
	month = nov,
	year = {2014},
	keywords = {Agricultural data collection, ALQIDCS limitation, Android, arable land quality data collection system, arable land quality index, arable land quality monitoring, arable land region, Beijing, China, data collection, data compatibility, data exchange interface structure, Data models, data table-driven model, Da Xing District, electronic data interchange, flexible data collection formula type, flexible data processing formula type, geographic information systems, geophysical techniques, highly flexible mobile GIS-based system development, index data collection, Indexes, Inner Mongolia, Mobile communication, mobile geographic information system (GIS), module partition, program modification, prototype system, quality index system, quality index system variation, self-adaptive method series, Spatial databases, spatial data content, spatial data index type utilization, spatial data variation, spatial quality index type utilization, Te DaLa Qi District, traditional method, traditional pen-and-paper method, Vectors, well-designed terminal-based method},
	pages = {4432--4441},
	file = {IEEE Xplore Abstract Record:/media/soedomoto/DATA/BIBLIOGRAPHY/storage/NUQH5ZIQ/6815642.html:text/html;IEEE Xplore Full Text PDF:/media/soedomoto/DATA/BIBLIOGRAPHY/storage/4M3XDKCI/Ye et al. - 2014 - Development of a Highly Flexible Mobile GIS-Based .pdf:application/pdf}
}

